"GET html/":{
	"handler": "serveDir",
	"dropParams": true,
	"headers":{"Content-Type": "text/html; charset=utf-8"}
},

"GET 404":{
	"handler":"headers",
	"status":404
},

"GET slow/":{
    "handler": "serveString",
    "headers":{"Content-Type": "text/html; charset=utf-8"},
    "delay": 3.0,
    "serve": "<html><body>Slow, isn't it?</body></html>"
},

"GET christ-mocked.jpg":{
	"handler": "proxy",
	"URL": "http://www.access-jesus.com/images/christ-mocked.jpg"
},

"GET":{
	"handler": "serveFile",
	"headers":{"Content-Type": "text/html; charset=utf-8"},
	"serve": "index.html"
}
